{
  "author": "bydeivih",
  "build": {
    "appId": "applio.iahispano.com",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "icon": "favicon.png",
      "target": "AppImage"
    },
    "mac": {
      "icon": "favicon.png",
      "target": "dmg"
    },
    "win": {
      "icon": "favicon.png",
      "target": [
        {
          "arch": [
            "x64"
          ],
          "target": "portable"
        }
      ]
    }
  },
  "dependencies": {
    "uaup-js": "^0.0.22"
  },
  "description": "Installer for Enhanced RVC Variant named Applio, now more easy, simple and fast.",
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4"
  },
  "license": "MIT",
  "main": "main.js",
  "name": "Applio-Installer",
  "scripts": {
    "start": "electron ."
    "dist": "electron-builder"
  },
  "version": "5.0.0"
}
